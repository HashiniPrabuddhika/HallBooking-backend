generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model mrbsroom {
  id          Int         @id @default(autoincrement())
  room_name   String      @unique(map: "MRBSRoom_room_name_key")
  description String?
  capacity    Int
  entries     mrbsentry[]
}

model mrbsentry {
  id           Int      @id @default(autoincrement())
  roomId       Int
  name         String
  booking_date String
  start_time   String
  end_time     String
  description  String?
  created_by   String
  modified_by  String
  room         mrbsroom @relation(fields: [roomId], references: [id], map: "MRBSEntry_roomId_fkey")

  @@index([roomId], map: "MRBSEntry_roomId_fkey")
  @@index([booking_date])
  @@index([start_time])
}
